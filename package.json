{
  "name": "skillbuilder_app",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "author": "Nikita Pednekar",
  "scripts": {
    "shell": "./build.sh",
    "start": "npm run serve",
    "build": "npm run build-ts",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "format": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix"
  },
  "dependencies": {
    "aws-sdk": "^2.1077.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "formidable": "^2.0.1",
    "generate-password": "^1.7.0",
    "google-auth-library": "^7.11.0",
    "googleapis": "^107.0.0",
    "http-status-codes": "^1.4.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "mime": "^3.0.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
    "newrelic": "^9.11.0",
    "node-cron": "^3.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "stripe": "^8.209.0",
    "typeorm": "0.2.25",
    "url-parse": "^1.5.1",
    "uuidv4": "^6.2.12",
    "winston": "^3.2.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/concurrently": "^5.2.0",
    "@types/cors": "^2.8.6",
    "@types/eslint": "^6.8.1",
    "@types/express": "^4.17.6",
    "@types/express-serve-static-core": "^4.17.18",
    "@types/formidable": "^2.0.5",
    "@types/generate-password": "^1.5.1",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.11",
    "@types/node": "^14.14.42",
    "@types/node-cron": "^3.0.0",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/request": "^2.48.5",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/url-parse": "^1.4.3",
    "@types/winston": "^2.4.4",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.3.2",
    "swagger-ui-express": "^4.1.4",
    "ts-node": "8.10.1",
    "typescript": "3.9.3",
    "yamljs": "^0.3.0"
  }
}
